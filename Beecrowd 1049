#include<stdio.h>
int main(){
      char S[15];
      char Y[15];
      char Z[15];
      scanf("%s %s %s",S,Y,Z);
      if(S[0]== 'v' && Y[0] == 'a' && Z[0] == 'c'){
          printf("aguia\n");
      }if(S[0]=='v' && Y[0]=='a' && Z[0]=='o'){
           printf("pomba\n");
      
       }if(S[0]=='v'&&Y[0]=='m'&&Z[0]=='o'){
           printf("homem\n");
       }if(S[0]=='v'&&Y[0]=='m'&&Z[0]=='h'){
           printf("vaca\n");
        }if(S[0]=='i'&&Y[0]=='i'&&Z[2]=='m'){
           printf("pulga\n");
      }if(S[0]=='i'&&Y[0]=='i'&&Z[2]=='r'){
           printf("lagarta\n");

      }if(S[0]=='i'&&Y[0]=='a'&&Z[0]=='h'){
          printf("sanguessuga\n");
      }if(S[0]=='i'&&Y[0]=='a'&&Z[0]=='o'){
          printf("minhoca\n");
      }
      
       return 0;
}
