#include<stdio.h>
int main(){
   int N,sum=0,count=0,total=0;
   while(1){
      scanf("%d",&N);

      if(N==1){
         sum=sum+1;

      }else if(N==2){
         count=count+1;
      }else if(N==3){
       total=total+1;
      }else if(N==4){
         break;
      }
   }
      printf("MUITO OBRIGADO\n");
      printf("Alcool: %d\n",sum);
      printf("Gasolina: %d\n",count);
      printf("Diesel: %d\n",total);
   
     return 0;
}
    
