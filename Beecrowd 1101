#include<stdio.h>
int main(){
   int M,N,c=0;
   while(1){
    scanf("%d %d",&M,&N);
    if(M<=0||N<=0){
      break;
    }else{
      c=0;
     if(M<N){
      for(int i=M;i<=N;i++){
        printf("%d ",i);
        c+=i;
      }
       printf("Sum=%d\n",c);
     }else if(M>N){
      for(int i=N;i<=M;i++){
        printf("%d ",i);
        c+=i;
      }
       printf("Sum=%d\n",c);
     }
    }
   }
     return 0;
}
